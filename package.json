{
  "name": "Neon",
  "version": "2.6.2",
  "main": "./main.js",
  "description": "Light wallet for NEO blockchain",
  "homepage": "https://github.com/CityOfZion/neon-wallet",
  "author": "Ethan Fast <ejhfast@gmail.com> (https://github.com/Ejhfast)",
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "start": "cross-env NODE_ENV=production electron .",
    "assets": "cross-env NODE_ENV=production webpack --config ./config/webpack.config.prod --progress",
    "dev": "cross-env START_HOT=1 NODE_ENV=development node_modules/.bin/webpack-dev-server --progress --config ./config/webpack.config.dev",
    "start-dev": "cross-env NODE_ENV=development electron . --enable-logging --remote-debugging-port=9222 --progress",
    "assets-dev": "cross-env NODE_ENV=development webpack --config ./config/webpack.config.dev --progress",
    "test": "npm rebuild node-hid && ./node_modules/.bin/jest --coverage",
    "test-ci": "yarn test --maxWorkers=4 --watchAll=false && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "test:e2e": "ava __e2e__/*.e2e.js --timeout=120s -s",
    "test-watch": "jest --watch",
    "pack": "build --dir",
    "dist": "yarn assets && dotenv yarn electron-builder --publish",
    "lint": "eslint -c .eslintrc ./app --ext .js,.jsx,.ts,.tsx --ignore-pattern dist/",
    "lint-fix": "eslint -c .eslintrc ./app --ext .js,.jsx,.ts,.tsx --fix --ignore-pattern dist/",
    "prepush": "echo 'running type check...' && yarn typecheck && echo 'running lint...' && yarn lint && echo 'running tests...' && yarn test",
    "electron-rebuild": "electron-rebuild --force --module_dir . -w node-hid",
    "typecheck": "tsc --version && tsc -b",
    "format": "prettier --write '**/*.{js,jsx,.scss,.css}'",
    "clean": "rm -r app/dist/*"
  },
  "keywords": [],
  "license": "MIT",
  "build": {
    "appId": "com.electron.neon",
    "files": [
      "./preload.js",
      "./main.js",
      "app/dist/index.html",
      "app/dist/bundle.js",
      "app/dist/style.css"
    ],
    "afterSign": "electron-builder-notarize",
    "publish": [
      {
        "provider": "github",
        "owner": "CityOfZion",
        "repo": "neon-wallet"
      }
    ],
    "dmg": {
      "background": "./app/assets/images/background.tiff",
      "window": {
        "width": 576,
        "height": 404
      },
      "contents": [
        {
          "x": 168,
          "y": 200
        },
        {
          "x": 408,
          "y": 200,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "mac": {
      "hardenedRuntime": true,
      "entitlements": "mac-config/entitlements.mac.plist",
      "entitlementsInherit": "mac-config/entitlements.mac.plist",
      "artifactName": "${productName}.${version}.${ext}"
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ],
      "artifactName": "${productName}.${version}.${ext}"
    },
    "win": {
      "target": "nsis",
      "icon": "build/icon.ico",
      "artifactName": "${productName}.${version}.${ext}"
    }
  },
  "dependencies": {
    "@babel/plugin-transform-runtime": "^7.13.10",
    "@babel/preset-env": "^7.13.12",
    "@babel/preset-react": "^7.13.13",
    "@babel/preset-stage-0": "^7.8.3",
    "@babel/preset-typescript": "^7.13.0",
    "@cityofzion/neon-js": "3.11.9",
    "@formatjs/intl-pluralrules": "^1.5.2",
    "@ledgerhq/hw-transport-node-hid": "5.0.0",
    "@types/enzyme": "^3.10.8",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^26.0.22",
    "@types/mocha": "^8.2.2",
    "@types/react-dom": "^17.0.3",
    "@types/react-redux": "^7.1.16",
    "@types/react-router-dom": "^5.1.7",
    "@types/redux-logger": "^3.0.8",
    "@types/redux-mock-store": "^1.0.2",
    "@types/webpack-env": "^1.16.0",
    "@typescript-eslint/eslint-plugin": "^4.20.0",
    "@typescript-eslint/parser": "^4.20.0",
    "axios": "0.21.1",
    "axios-retry": "3.1.1",
    "babel-eslint": "^10.0.0",
